# terraform.tfvars.example - Copy to terraform.tfvars and customize

# AWS region (us-east-1 required for CloudFront ACM certificates)
aws_region = "us-east-1"

# Project prefix for namespacing resources (bucket: {prefix}-static, etc.)
prefix = "canonry"

# Domain name (Route53 hosted zone must already exist)
domain_name = "the-canonry.com"
